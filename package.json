{
  "name": "jobjorns-ordspel",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3500",
    "build": "next build",
    "vercel-build": "prisma generate && prisma db push && next build",
    "start": "next start -p 10000",
    "what": "ECHO Package system: npm",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "prisma:pull:local": "dotenv -e .env.local -- npx prisma db pull",
    "prisma:push:local": "dotenv -e .env.local -- npx prisma db push",
    "prisma:generate:local": "dotenv -e .env.local -- npx prisma generate",
    "prisma:migrate:dev": "dotenv -e .env.local -- prisma migrate dev",
    "cron-reminders": "tsc ./cron/reminders.ts --outDir ./dist/ && dotenv -e .env.local -- node ./dist/reminders.js"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^3.5.0",
    "@emotion/react": "^11.13.3",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.1.2",
    "@mui/material": "^6.1.2",
    "@mui/system": "^6.1.2",
    "@prisma/client": "^5.20.0",
    "@sendgrid/mail": "^8.1.3",
    "ably": "^2.4.1",
    "chart.js": "^4.4.4",
    "he": "^1.2.0",
    "luxon": "^3.5.0",
    "next": "^14.2.14",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.2.0",
    "react-confetti": "^6.1.0",
    "react-dom": "^18.3.1",
    "vite-tsconfig-paths": "^5.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/he": "^1.2.3",
    "@types/jest": "^29.5.13",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.7.4",
    "@types/react": "^18.3.11",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.14",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "prisma": "^5.20.0",
    "typescript": "^5.6.2",
    "vitest": "^2.1.2"
  }
}
